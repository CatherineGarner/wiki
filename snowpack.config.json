{
    "mount": {
      "_output": "/",
      "src": "/_dist_"
    },
    "plugins": [
      [
        "@snowpack/plugin-run-script",
        { "cmd": "eleventy", "watch": "$1 --watch" }
      ],
        ["@snowpack/plugin-build-script", { "cmd": "postcss", "input": [".css"], "output": [".css"]}]
    ]
  }
  